@model Employee

<h2>Details</h2>
<table class=" table table-bordered tabletable-hover ">

    <tr>
        <td>@Model.Id</td>
        <td>@Model.Name</td>
   
        <td>@Model.Salary</td>

        <td>@Model.ImageUrl</td>
        <td>@Model.JobTitle</td>
        <td>@Model.Address</td>
    </tr>


</table>

<h3> @ViewData["Mes"]</h3>
<h3> @(int.Parse(ViewData["Temp"].ToString())-10) </h3>

<select class="form form-control">
    @foreach (var item in (List<string>)ViewData["brch"]) 
    {
        <option> @item</option>
        
    }
</select>
