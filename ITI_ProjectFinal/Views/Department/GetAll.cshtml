@model List<Department>

@{
    //ViewData["Titel"] = "Index";
    Layout = "_AdvanxedLayout";
}

<table class="table table-bordered table-hover" style="background-color:aqua , black">
    <a href="New"></a> 
    <tr>
        <th>No</th>
        <th>Name</th>
    </tr>
    @foreach (var item in Model)
    { 
       
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ManagerName</td>
           
        </tr>
        
    }
    
</table>